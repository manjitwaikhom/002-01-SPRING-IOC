<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd"
	xmlns:mjt="http://www.springframework.org/schema/p"
	xmlns:context="http://www.springframework.org/schema/context">

<context:annotation-config/>

<bean id="hello2" class="com_007.spring.annotationautowiring.Hello" mjt:a="99" mjt:str="Hello guys"/>
<bean id="hai1" class="com_007.spring.annotationautowiring.Hai">
<constructor-arg value="88"/>
<constructor-arg value="Hai Guys"/>
</bean>
<bean id="aobj" class="com_007.spring.annotationautowiring.A"/>
</beans>