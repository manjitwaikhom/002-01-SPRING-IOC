<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="hai" class="com_003.spring.autowirebyname.Hai">
<property name="msg" value="Welcome to Spring3 Tutorial-Hai"/>
<property name="no" value="99"/>
</bean>

<bean id="aobj" class="com_003.spring.autowirebyname.A">
<property name="msg" value="Welcome to  Spring3 Tutorial-A"/>
</bean>

<bean id="hello" class="com_003.spring.autowirebyname.Hello" autowire="byName"/>
</beans>